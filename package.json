  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config prod.config.ts",
    "build:test": "cross-env NODE_ENV=production webpack --config test.config.ts",
    "dev": "cross-env NODE_ENV=production webpack serve --config dev.config.ts",
    "preinstall": "npx only-allow pnpm",
    "lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
