    "css-loader": "6.7.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.8.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "2.7.2",
    "postcss": "8.4.20",
    "postcss-loader": "7.0.2",
